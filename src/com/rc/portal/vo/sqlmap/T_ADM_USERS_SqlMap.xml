  <select id="modulelist" resultMap="modulelistMap" parameterClass="java.util.Map" >
			select a.id,a.module_name,a.parent_id,a.editable,c.userid from t_adm_modules a
			left join t_adm_modules d on d.id=a.parent_id      
			left join (select * from t_adm_users_modules b where 1=1 
						<isNotNull property="userid">
							and b.userid=$userid$
						</isNotNull>								 
					 ) c on a.id=c.moduleid
			where 1=1
			<isNotNull property="webid">
				and a.webid=$webid$
			</isNotNull>
			<isNotNull property="name">
				and (d.module_name  like  '%$name$%' or a.module_name  like  '%$name$%')
			</isNotNull>
  </select>
  
