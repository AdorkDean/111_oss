  	<select id="selectPharmacistCommentListMap" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
  		SELECT tp.id,tp.pharmacist_id AS pctid,tp.comment_content AS content,tp.is_view,tp.goodsid
  		FROM t_pharmacist AS tp 
  		WHERE tp.goodsid = $goodsid$ 
  		ORDER BY tp.id DESC 
  		LIMIT 1
	</select>
	
  	<select id="selectphysicianListMap" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
  		SELECT tn.id, tn.real_name 
  		FROM t_physician tn 
  		ORDER BY tn.id DESC 
  		LIMIT 10
	</select>
	
  	<select id="selectDatasByPage" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
       SELECT  tst.id,
		       tp.head_url AS phyheaderurl,
		       tp.real_name AS realname,
		       tst.comment_content AS content,
		       tst.comment_time AS ctime,
		       tst.is_view AS isview,
		       tst.goodsid,
		       tg.short_name AS shortname,
		       tg.goodscode AS sn,
		       tg.is_pc,
       		   tg.is_wap,
               tg.is_app,
               tg.goodsno
		       FROM t_pharmacist AS tst
		       LEFT JOIN t_physician AS tp ON tp.id = tst.pharmacist_id
		       LEFT JOIN t_goods AS tg ON tg.id = tst.goodsid
       WHERE 1 = 1
       <isNotNull prepend="and" property="goodsno" >
       tg.goodsno = #goodsno#
      </isNotNull>
       ORDER BY tst.id DESC 
       LIMIT $pageInfo.start$ ,$pageInfo.pageSize$
	</select>
	
