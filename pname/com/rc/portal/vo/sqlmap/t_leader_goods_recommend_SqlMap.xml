   <select id="select_leader_goods_count" resultClass="java.lang.Integer" parameterClass="java.util.Map">
        SELECT
		count(*)
		FROM t_leader_goods_recommend t
		LEFT JOIN t_goods t1 ON t.goods_id = t1.id
		LEFT JOIN t_goods_brokerage t2 ON t.goods_id = t2.goods_id
		 where t.type=#type#
		    <isNotNull prepend="and" property="goodsName">
				t1.goods_name like '%$goodsName$%'
			</isNotNull>
			<isNotNull prepend="and" property="goodsno">
				t1.goodsno like '%$goodsno$%'
			</isNotNull>
		ORDER BY t.weight DESC
  
  </select>
  
	<select id="select_leader_goods_list" resultClass="java.util.HashMap"
		parameterClass="java.util.Map">
		SELECT
		t.id,
		t1.goodsno,
		t1.goods_name,
    TRUNCATE(IFNULL(t2.rebate_amount,tgp.price*(#brokerage#)/100),2) AS rebate_amount
		FROM t_leader_goods_recommend t
		LEFT JOIN t_goods t1 ON t.goods_id = t1.id
    LEFT JOIN t_goods_price tgp ON t.goods_id = tgp.goods_id AND tgp.platform_type='111yao' AND tgp.price_type='wap'
		LEFT JOIN t_goods_brokerage t2 ON t.goods_id = t2.goods_id AND t2.audit_status=2
		 where t.type=#type#
		    <isNotNull prepend="and" property="goodsName">
				t1.goods_name like '%$goodsName$%'
			</isNotNull>
			<isNotNull prepend="and" property="goodsno">
				t1.goodsno like '%$goodsno$%'
			</isNotNull>
		ORDER BY t.weight DESC
		limit $pageInfo.start$ ,$pageInfo.pageSize$
	</select>
     
	<select id="select_t_goods_brokerage_bygoodsno" resultClass="java.util.HashMap"
		parameterClass="java.util.Map">
		  SELECT
			t.goods_id,
	      TRUNCATE(IF(t.audit_status=2,t.rebate_amount,tgp.price*(#brokerage#)/100),2) AS rebate_amount,
			  t1.goods_name,
			  t1.goodsno
			FROM
				t_goods_brokerage t
			LEFT JOIN t_goods t1 ON t.goods_id = t1.id
	    LEFT JOIN t_goods_price tgp ON t.goods_id = tgp.goods_id AND tgp.platform_type='111yao' AND tgp.price_type='wap'  
		 where t1.goodsno = '$goodsno$'
	</select>
     
     
