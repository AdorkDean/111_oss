  <select id="selectCouponCardListByMap" resultClass="java.util.HashMap" parameterClass="java.util.Map">
	select t.*,m.user_name,c.`name` as couponName,t.start_time,t.end_time from t_coupon_card t
left JOIN t_member m on m.id=t.member_id
LEFT JOIN t_coupon c on c.id=t.ticket_id
where t.ticket_id=#couponid#
      order by t.create_time desc
      <isNotNull property="type">
      	<isEqual property="type" compareValue="0">
     LIMIT $pageInfo.start$,$pageInfo.end$
      	</isEqual>
      </isNotNull>
  </select>
