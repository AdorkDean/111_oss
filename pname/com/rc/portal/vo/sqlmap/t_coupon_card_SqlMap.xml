left JOIN t_member m on m.id=t.member_id
LEFT JOIN t_coupon c on c.id=t.ticket_id
where t.ticket_id=#couponid#
  </select>
 <select id="selectCouponCardByCardNo" parameterClass="java.util.Map" resultClass="com.rc.portal.vo.TCouponCard">
  select t.* FROM t_coupon_card t
left join t_coupon c ON c.id=t.ticket_id
where t.member_id is null 
AND c.start_time &lt;= NOW() AND c.end_time &gt;= NOW()
 and t.card_no=#cardNo#
  </select>
  
  
