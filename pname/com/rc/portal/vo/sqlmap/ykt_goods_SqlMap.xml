<!--   医卡通商品列表   杨洁亮 -->
 <select id="yktGoodsListByPage" parameterClass="java.util.Map" resultClass="java.util.HashMap" >
  select yg.id,yg.goods_no,yg.create_time,g.goods_name from ykt_goods yg
right JOIN t_goods g ON g.goodsno=yg.goods_no
where 1=1
<isNotNull prepend="and" property="goodsno">
	yg.goods_no=#goodsno#
</isNotNull>
<isNotNull prepend="and" property="goodsname">
	g.goods_name='%$goodsname$%'
</isNotNull>
ORDER BY yg.create_time DESC 
LIMIT  $pageInfo.start$ ,$pageInfo.end$
</select>


